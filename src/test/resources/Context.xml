<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <bean id="simpleBean" class="spring.Bean.SimpleBean">
        <property name="message" value="SimpleBean"></property>
    </bean>
    <bean id="prototypeBean" class="spring.Bean.SimpleBean" scope="prototype">
        <property name="message" value="Prototype"></property>
    </bean>
    <context:component-scan base-package="spring.Bean" ></context:component-scan>
</beans>